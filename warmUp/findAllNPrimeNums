function findAllNPrimeNums (nums) {
    let res = [];
    for(let i = 0;i<nums.length;i++) {
        if(nums[i] == 2) res.push(nums[i]);
        
        if(nums[i] %2 == 0) continue
        for(let k = 3;k<=nums[i];k++) {
            if(nums[i]%k == 0 ) res.push(nums[i])
        }
    }
    return res
}

console.log(findAllNPrimeNums([1,2,3,4,5,6,7,29]))